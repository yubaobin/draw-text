{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "build/**/*",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "types/*"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "#/*": [
        "./types/*"
      ]
    }
  }
}